{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nimport { useStore } from \"vuex\";\nimport axios from \"axios\";\nimport Cookies from \"js-cookie\";\nimport jwtDecode from \"jwt-decode\";\nexport default {\n  setup() {\n    let isLogin = ref(false);\n    let store = useStore();\n    let user = ref(\"\");\n    let userId = ref();\n    onMounted(() => {\n      // console.log(this.token.value);\n      axios.get(\"http://localhost:3000/check\", {\n        withCredentials: true\n      }).then(response => {\n        if (response.data.isLogin === true) {\n          store.commit(\"login\");\n          // console.log(store.state.isLogin);\n          isLogin.value = store.state.isLogin;\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n      const token = Cookies.get(\"token\");\n      if (token) {\n        user.value = jwtDecode(token);\n        userId.value = user.value.id;\n        console.log(userId.value);\n      }\n    });\n    return {\n      isLogin,\n      user,\n      userId\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","useStore","axios","Cookies","jwtDecode","setup","isLogin","store","user","userId","get","withCredentials","then","response","data","commit","value","state","catch","error","console","log","token","id"],"sources":["/home/prism/Documents/flyticket/src/views/UserProfile.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"user-profile\"></div>\n    <div class=\"user-info\"></div>\n  </div>\n</template>\n<script>\nimport { ref, onMounted } from \"vue\";\nimport { useStore } from \"vuex\";\nimport axios from \"axios\";\nimport Cookies from \"js-cookie\";\nimport jwtDecode from \"jwt-decode\";\nexport default {\n  setup() {\n    let isLogin = ref(false);\n    let store = useStore();\n    let user = ref(\"\");\n    let userId = ref();\n    onMounted(() => {\n      // console.log(this.token.value);\n      axios\n        .get(\"http://localhost:3000/check\", { withCredentials: true })\n        .then((response) => {\n          if (response.data.isLogin === true) {\n            store.commit(\"login\");\n            // console.log(store.state.isLogin);\n            isLogin.value = store.state.isLogin;\n          }\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n      const token = Cookies.get(\"token\");\n      if (token) {\n        user.value = jwtDecode(token);\n        userId.value = user.value.id;\n        console.log(userId.value);\n      }\n    });\n    return { isLogin, user, userId };\n  },\n};\n</script>\n<style>\n.container {\n  margin: auto;\n  padding: auto;\n  gap: 2rem\n  display: flex;\n  flex-direction: row;\n}\n.user-profile {\n  width: 30%;\n  height: 70rem;\n  border: 1rem red solid;\n}\n.user-info {\n  width: 60%;\n  height: 70rem;\n  border: 1rem red solid;\n}\n</style>\n"],"mappings":"AAOA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,QAAO,QAAS,MAAM;AAC/B,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,WAAW;AAC/B,OAAOC,SAAQ,MAAO,YAAY;AAClC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,IAAIC,OAAM,GAAIP,GAAG,CAAC,KAAK,CAAC;IACxB,IAAIQ,KAAI,GAAIN,QAAQ,EAAE;IACtB,IAAIO,IAAG,GAAIT,GAAG,CAAC,EAAE,CAAC;IAClB,IAAIU,MAAK,GAAIV,GAAG,EAAE;IAClBC,SAAS,CAAC,MAAM;MACd;MACAE,KAAI,CACDQ,GAAG,CAAC,6BAA6B,EAAE;QAAEC,eAAe,EAAE;MAAK,CAAC,EAC5DC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAIA,QAAQ,CAACC,IAAI,CAACR,OAAM,KAAM,IAAI,EAAE;UAClCC,KAAK,CAACQ,MAAM,CAAC,OAAO,CAAC;UACrB;UACAT,OAAO,CAACU,KAAI,GAAIT,KAAK,CAACU,KAAK,CAACX,OAAO;QACrC;MACF,CAAC,EACAY,KAAK,CAAEC,KAAK,IAAK;QAChBC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC,CAAC;MACJ,MAAMG,KAAI,GAAInB,OAAO,CAACO,GAAG,CAAC,OAAO,CAAC;MAClC,IAAIY,KAAK,EAAE;QACTd,IAAI,CAACQ,KAAI,GAAIZ,SAAS,CAACkB,KAAK,CAAC;QAC7Bb,MAAM,CAACO,KAAI,GAAIR,IAAI,CAACQ,KAAK,CAACO,EAAE;QAC5BH,OAAO,CAACC,GAAG,CAACZ,MAAM,CAACO,KAAK,CAAC;MAC3B;IACF,CAAC,CAAC;IACF,OAAO;MAAEV,OAAO;MAAEE,IAAI;MAAEC;IAAO,CAAC;EAClC;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}