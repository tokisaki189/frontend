 => {\n          orders.value = response.data;\n          console.log(orders.value);\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    }\n    return {\n      orders,\n      approve,\n      deny,\n      getAll,\n      getPending,\n      getApproved,\n      getDenied,\n    };\n  },\n};\n</script>\n<style scoped>\n.container {\n  display: flex;\n  flex-direction: column;\n}\n.order {\n}\nth,\ntd {\n  vertical-align: top;\n  border: 1px solid black;\n  width: 100px;\n}\ntr {\n  vertical-align: top;\n}\ntr:nth-child(even) {\n  background-color: #d6d6d6;\n}\ntable {\n  width: 100%;\n  border-spacing: 3px;\n  height: 40rem;\n}\nbutton {\n  width: 50%;\n  height: 100%;\n  border: none;\n}\n.approve {\n  background-color: greenyellow;\n}\n.deny {\n  background-color: red;\n}\n.button-tab-container {\n  display: flex;\n  flex-direction: row;\n}\n.tab {\n  height: 2rem;\n}\n</style>\n"],"mappings":"AAuDA,SAASA,SAAQ,QAAS,KAAK;AAC/B,OAAOC,GAAE,MAAO,WAAW;AAC3B,SAASC,GAAE,QAAS,KAAK;AACzB,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,KAAKA,CAAA,EAAG;IACN,IAAIC,MAAK,GAAIH,GAAG,CAAC,EAAE,CAAC;IACpBF,SAAS,CAAC,MAAM;MACdC,GAAE,CACCK,GAAG,CAAC,YAAY,EAChBC,IAAI,CAAEC,QAAQ,IAAK;QAClBH,MAAM,CAACI,KAAI,GAAID,QAAQ,CAACE,IAAI;QAC5BC,OAAO,CAACC,GAAG,CAACP,MAAM,CAACI,KAAK,CAAC;MAC3B,CAAC,EACAI,KAAK,CAAEC,KAAK,IAAK;QAChBH,OAAO,CAACG,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN,CAAC,CAAC;IACF,SAASC,OAAOA,CACdC,QAAQ,EACRC,OAAO,EACPC,iBAAiB,EACjBC,mBAAkB,EAClB;MACAR,OAAO,CAACC,GAAG,CAAC,SAAQ,GAAII,QAAO,GAAI,UAAS,GAAIC,OAAO,CAAC;MACxDhB,GAAG,CAACmB,GAAG,CAAC,WAAU,GAAIJ,QAAQ,EAAE;QAC9BK,MAAM,EAAEJ,OAAO;QACfK,UAAU,EAAEJ,iBAAiB;QAC7BK,YAAY,EAAEJ;MAChB,CAAC,CAAC;IACJ;IACA,SAASK,IAAIA,CAACR,QAAQ,EAAEC,OAAO,EAAE;MAC/BN,OAAO,CAACC,GAAG,CAAC,SAAQ,GAAII,QAAO,GAAI,UAAS,GAAIC,OAAO,CAAC;MACxDhB,GAAG,CAACmB,GAAG,CAAC,QAAO,GAAIJ,QAAQ,EAAE;QAC3BK,MAAM,EAAEJ;MACV,CAAC,CAAC;IACJ;IACA,SAASQ,MAAMA,CAAA,EAAG;MAChBxB,GAAE,CACCK,GAAG,CAAC,YAAY,EAChBC,IAAI,CAAEC,QAAQ,IAAK;QAClBH,MAAM,CAACI,KAAI