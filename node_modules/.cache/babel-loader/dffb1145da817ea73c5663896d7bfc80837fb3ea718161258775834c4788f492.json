{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nimport { useStore } from \"vuex\";\nimport Cookies from \"js-cookie\";\nimport jwtDecode from \"jwt-decode\";\nimport api from \"../api.js\";\nexport default {\n  setup() {\n    let isLogin = ref(false);\n    let store = useStore();\n    let user = ref(\"\");\n    let userId = ref();\n    onMounted(() => {\n      // console.log(this.token.value);\n      api.get(\"/check\", {\n        withCredentials: true\n      }).then(response => {\n        if (response.data.isLogin === true) {\n          store.commit(\"login\");\n          isLogin.value = store.state.isLogin;\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n      const token = Cookies.get(\"token\");\n      if (token) {\n        user.value = jwtDecode(token);\n        userId.value = user.value.id;\n        console.log(userId.value);\n        api.get(\"/userprofile/\" + userId.value, {}).then(response => {\n          user.value = response.data;\n          console.log(user.value);\n        }).catch(error => {\n          console.log(error);\n        });\n      }\n    });\n    return {\n      isLogin,\n      user,\n      userId\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","useStore","Cookies","jwtDecode","api","setup","isLogin","store","user","userId","get","withCredentials","then","response","data","commit","value","state","catch","error","console","log","token","id"],"sources":["/home/prism/Documents/flyticket/src/views/UserProfile.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"user-profile\"></div>\n    <div class=\"user-info\">\n      <p></p>\n\n    </div>\n  </div>\n</template>\n<script>\nimport { ref, onMounted } from \"vue\";\nimport { useStore } from \"vuex\";\nimport Cookies from \"js-cookie\";\nimport jwtDecode from \"jwt-decode\";\nimport api from \"../api.js\";\nexport default {\n  setup() {\n    let isLogin = ref(false);\n    let store = useStore();\n    let user = ref(\"\");\n    let userId = ref();\n    onMounted(() => {\n      // console.log(this.token.value);\n      api\n        .get(\"/check\", { withCredentials: true })\n        .then((response) => {\n          if (response.data.isLogin === true) {\n            store.commit(\"login\");\n            isLogin.value = store.state.isLogin;\n          }\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n      const token = Cookies.get(\"token\");\n      if (token) {\n        user.value = jwtDecode(token);\n        userId.value = user.value.id;\n        console.log(userId.value);\n        api\n          .get(\"/userprofile/\" + userId.value, {})\n          .then((response) => {\n            user.value = response.data;\n            console.log(user.value);\n          })\n          .catch((error) => {\n            console.log(error);\n          });\n      }\n    });\n    return { isLogin, user, userId };\n  },\n};\n</script>\n<style scoped>\n.container {\n  margin: auto;\n  padding: auto;\n  margin-top: 5rem;\n  margin-left: 4rem;\n  gap: 2rem;\n  display: flex;\n  flex-direction: row;\n}\n.user-profile {\n  width: 20%;\n  height: 50rem;\n  border: 0.2rem rgb(0, 0, 0) solid;\n  border-radius: 2rem;\n}\n.user-info {\n  width: 60%;\n  height: 50rem;\n  border: 0.2rem rgb(0, 0, 0) solid;\n  border-radius: 2rem;\n}\n</style>\n"],"mappings":"AAUA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,QAAO,QAAS,MAAM;AAC/B,OAAOC,OAAM,MAAO,WAAW;AAC/B,OAAOC,SAAQ,MAAO,YAAY;AAClC,OAAOC,GAAE,MAAO,WAAW;AAC3B,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,IAAIC,OAAM,GAAIP,GAAG,CAAC,KAAK,CAAC;IACxB,IAAIQ,KAAI,GAAIN,QAAQ,EAAE;IACtB,IAAIO,IAAG,GAAIT,GAAG,CAAC,EAAE,CAAC;IAClB,IAAIU,MAAK,GAAIV,GAAG,EAAE;IAClBC,SAAS,CAAC,MAAM;MACd;MACAI,GAAE,CACCM,GAAG,CAAC,QAAQ,EAAE;QAAEC,eAAe,EAAE;MAAK,CAAC,EACvCC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAIA,QAAQ,CAACC,IAAI,CAACR,OAAM,KAAM,IAAI,EAAE;UAClCC,KAAK,CAACQ,MAAM,CAAC,OAAO,CAAC;UACrBT,OAAO,CAACU,KAAI,GAAIT,KAAK,CAACU,KAAK,CAACX,OAAO;QACrC;MACF,CAAC,EACA