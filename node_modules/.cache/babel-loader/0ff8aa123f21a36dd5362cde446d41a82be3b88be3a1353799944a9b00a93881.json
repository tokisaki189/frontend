          console.log(error);\n        });\n    });\n    function getAll() {\n      api\n        .get(\"/orderlist/\" + userId.value)\n        .then((response) => {\n          orders.value = response.data;\n          console.log(orders.value);\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    }\n    function getPending() {\n      api\n        .get(\"/pending/\" + userId.value)\n        .then((response) => {\n          orders.value = response.data;\n          console.log(orders.value);\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    }\n    function getApproved() {\n      api\n        .get(\"/approved/\" + userId.value)\n        .then((response) => {\n          orders.value = response.data;\n          console.log(orders.value);\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    }\n    function getDenied() {\n      api\n        .get(\"/denied/\" + userId.value)\n        .then((response) => {\n          orders.value = response.data;\n          console.log(orders.value);\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    }\n\n    return {\n      orders,\n      user,\n      userId,\n      getApproved,\n      getDenied,\n      getPending,\n      getAll,\n      statusApproved,\n    };\n  },\n};\n</script>\n<style scoped>\n.container {\n  display: flex;\n  flex-direction: column;\n  margin-top: 10rem;\n}\n.order {\n}\n</style>\n"],"mappings":"AAmCA,OAAOA,GAAE,MAAO,QAAQ;AACxB,SAASC,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,QAAO,QAAS,MAAM;AAC/B,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,KAAKA,CAAA,EAAG;IACN,IAAIC,KAAI,GAAIH,QAAQ,EAAE;IACtB,IAAII,IAAG,GAAIN,GAAG,CAAC,EAAE,CAAC;IAClB,IAAIO,MAAK,GAAIP,GAAG,EAAE;IAClB,IAAIQ,MAAK,GAAIR,GAAG,CAAC,EAAE,CAAC;IACpB,IAAIS,cAAa,GAAIT,GAAG,CAAC,EAAE,CAAC;IAC5BC,SAAS,CAAC,MAAM;MACdM,MAAM,CAACG,KAAI,GAAIL,KAAK,CAACM,KAAK,CAACC,MAAM;MACjCC,OAAO,CAACC,GAAG,CAACT,KAAK,CAACM,KAAK,CAACC,MAAM,