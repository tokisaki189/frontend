.value.slice(start, end);\n    });\n    function previousPage() {\n      if (currentPage.value > 0) {\n        currentPage.value--;\n      }\n    }\n    function nextPage() {\n      if (currentPage.value < Math.floor(flights.value.length / pageSize)) {\n        currentPage.value++;\n      }\n    }\n    return {\n      addflight,\n      flights,\n      formatDate,\n      previousPage,\n      nextPage,\n      displayedItems,\n      plane,\n      departureCity,\n      arrivalCity,\n      departureDate,\n      departureTime,\n      arrivalTime,\n      arrivalDate,\n      normalSeat,\n      normalPrice,\n      businessSeat,\n      businessPrice,\n      page\n    };\n  }\n};","map":{"version":3,"names":["api","ref","onMounted","computed","name","setup","flights","currentPage","page","plane","departureCity","arrivalCity","departureDate","departureTime","arrivalTime","arrivalDate","normalSeat","normalPrice","businessSeat","businessPrice","pageSize","get","then","response","value","data","console","log","catch","error","addflight","post","formatDate","date","dateObj","Date","year","getFullYear","month","getMonth","day","getDate","displayedItems","start","end","slice","previousPage","nextPage","Math","floor","length"],"sources":["/home/prism/Documents/flyticket/src/components/Admin/FlightCrud.vue"],"sourcesContent":["<template>\n  <div>crud</div>\n  <div class=\"container\">\n    <form class=\"form-container\">\n      <label for=\"plane\">Plane</label>\n      <input type=\"text\" name=\"\" id=\"plane\" v-model=\"plane\" />\n      <div class=\"city-container\">\n        <label for=\"deparute_city\">Departure City</label>\n        <input\n          type=\"text\"\n          name=\"\"\n          id=\"departure_city\"\n          v-model=\"departureCity\"\n        />\n        <label for=\"arrival_city\">Arrival City</label>\n        <input type=\"text\" name=\"\" id=\"arrival_city\" v-model=\"arrivalCity\" />\n      </div>\n      <div class=\"date-container\">\n        <label for=\"departure_date\"> Departure Date </label>\n        <input\n          type=\"date\"\n          name=\"\"\n          id=\"departure_date\"\n          v-model=\"departureDate\"\n        />\n        <label for=\"arrival_date\"> Arrival Date </label>\n        <input type=\"date\" name=\"\" id=\"arrival_date\" v-model=\"arrivalDate\" />\n      </div>\n      <div class=\"time-container\">\n        <label for=\"departure_time\">Departure Time</label>\n        <input\n          type=\"time\"\n          name=\"\"\n          id=\"departure_time\"\n          v-model=\"departureTime\"\n        />\n        <label for=\"arrival_time\">Arrival Time</label>\n        <input type=\"time\" name=\"\" id=\"arrival_time\" v-model=\"arrivalTime\" />\n      </div>\n      <div class=\"normal-seat-container\">\n        <label for=\"normal_seat\">Normal Seat Available</label>\n        <input type=\"number\" name=\"\" id=\"normal_seat\" v-model=\"normalSeat\" />\n        <label for=\"normal_seat_price\"> Normal Seat Price</label>\n        <input\n          type=\"number\"\n          name=\"\"\n          id=\"normal_seat_price\"\n          v-model=\"normalPrice\"\n        />\n      </div>\n      <div class=\"business-seat-container\">\n        <label for=\"business_seat\">Business Seat Available</label>\n        <input\n          type=\"number\"\n          name=\"\"\n          id=\"business_seat\"\n          v-model=\"businessSeat\"\n        />\n        <label for=\"business_seat_price\">Business Seat Price</label>\n        <input\n          type=\"number\"\n          name=\"\"\n          id=\"business_seat_price\"\n          v-model=\"businessPrice\"\n        />\n      </div>\n      <button class=\"add-flight\" @click=\"addflight\">Add Flight</button>\n    </form>\n  </div>\n  <div class=\"flight-list-container\">\n    <table class=\"flight-list\">\n      <tr>\n        <th>Plane Name</th>\n        <th>Departure City</th>\n        <th>Arrival City</th>\n        <th>Departure Date</th>\n        