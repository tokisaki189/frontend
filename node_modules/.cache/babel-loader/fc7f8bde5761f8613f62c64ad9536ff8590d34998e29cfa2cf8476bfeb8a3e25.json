{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nimport FlightCrud from \"../components/Admin/FlightCrud.vue\";\nimport OrderApproval from \"../components/Admin/FlightApproval.vue\";\nimport DashBoard from \"../components/Admin/DashBoard.vue\";\nexport default {\n  components: {\n    FlightCrud,\n    OrderApproval,\n    DashBoard\n  },\n  setup() {\n    let tabs = [\"FlightCrud\", \"OrderApproval\", \"DashBoard\"];\n    let selected = ref(tabs[0]);\n    let activeTabIndex = ref(null);\n\n    // Set activeTabIndex based on selected component\n    onMounted(() => {\n      activeTabIndex.value = tabs.indexOf(selected.value);\n    });\n\n    // Store selected component in localStorage when it changes\n    const storeSelectedComponent = () => {\n      localStorage.setItem(\"selected\", selected.value);\n    };\n    return {\n      tabs,\n      selected,\n      activeTabIndex,\n      storeSelectedComponent\n    };\n  },\n  created() {\n    // Retrieve selected component from localStorage\n    const storedSelectedComponent = localStorage.getItem(\"selected\");\n    if (storedSelectedComponent && this.tabs.includes(storedSelectedComponent)) {\n      this.selected = storedSelectedComponent;\n    }\n  }\n};","map":{"version":3,"names":["ref","onMounted","FlightCrud","OrderApproval","DashBoard","components","setup","tabs","selected","activeTabIndex","value","indexOf","storeSelectedComponent","localStorage","setItem","created","storedSelectedComponent","getItem","includes"],"sources":["/home/prism/Documents/flyticket/src/views/AdminPanel.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"button-container\">\n      <p>Dashboard</p>\n      <button\n        class=\"tab-menu\"\n        v-for=\"(tab, index) in tabs\"\n        :key=\"tab\"\n        @click=\"\n          (selected = tab), (activeTabIndex = index);\n          storeSelectedComponent();\n        \"\n      >\n        {{ tab }}\n      </button>\n    </div>\n    <div class=\"component-container\">\n      <KeepAlive>\n        <component :is=\"selected\"></component>\n      </KeepAlive>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, onMounted } from \"vue\";\nimport FlightCrud from \"../components/Admin/FlightCrud.vue\";\nimport OrderApproval from \"../components/Admin/FlightApproval.vue\";\nimport DashBoard from \"../components/Admin/DashBoard.vue\";\n\nexport default {\n  components: {\n    FlightCrud,\n    OrderApproval,\n    DashBoard,\n  },\n  setup() {\n    let tabs = [\"FlightCrud\", \"OrderApproval\", \"DashBoard\"];\n    let selected = ref(tabs[0]);\n    let activeTabIndex = ref(null);\n\n    // Set activeTabIndex based on selected component\n    onMounted(() => {\n      activeTabIndex.value = tabs.indexOf(selected.value);\n    });\n\n    // Store selected component in localStorage when it changes\n    const storeSelectedComponent = () => {\n      localStorage.setItem(\"selected\", selected.value);\n    };\n\n    return {\n      tabs,\n      selected,\n      activeTabIndex,\n      storeSelectedComponent,\n    };\n  },\n  created() {\n    // Retrieve selected component from localStorage\n    const storedSelectedComponent = localStorage.getItem(\"selected\");\n    if (\n      storedSelectedComponent &&\n      this.tabs.includes(storedSelectedComponent)\n    ) {\n      this.selected = storedSelectedComponent;\n    }\n  },\n};\n</script>\n\n<style scoped>\n.button-container {\n  display: flex;\n  flex-direction: column;\n  width: 15rem;\n  background-color: #2f81b7;\n}\n.component-container {\n  padding-top: 1rem;\n  width: 100%;\n  height: 100rem;\n  background-color: rgb(255, 255, 255);\n  box-shadow: 7px 12px #888888;\n  overflow: hidden;\n}\n.tab-menu {\n  font-size: larger;\n  height: 4rem;\n  border: none;\n  background-color: #2071a7;\n  background: linear-gradient(to right, #2d87c4, #2071a7);\n  background-size: 200% 100%;\n  transition: background-position 0.5s ease;\n  color: white;\n  font-size: larger;\n  font-weight: bold;\n}\n.tab-menu:hover {\n  /* background-color: #2d87c4; */\n  background-position: -100% 0;\n}\n.container {\n  display: flex;\n  margin-top: 5rem;\n}\n.button-container p {\n  height: 2rem;\n  font-size: x-large;\n  margin-top: 1rem;\n  color: white;\n  font-weight: bold;\n}\n</style>\n"],"mappings":"AAyBA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,OAAOC,UAAS,MAAO,oCAAoC;AAC3D,OAAOC,aAAY,MAAO,wCAAwC;AAClE,OAAOC,SAAQ,MAAO,mCAAmC;AAEzD,eAAe;EACbC,UAAU,EAAE;IACVH,UAAU;IACVC,aAAa;IACbC;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,IAAIC,IAAG,GAAI,CAAC,YAAY,EAAE,eAAe,EAAE,WAAW,CAAC;IACvD,IAAIC,QAAO,GAAIR,GAAG,CAACO,IAAI,CAAC,CAAC,CAAC,CAAC;IAC3B,IAAIE,cAAa,GAAIT,GAAG,CAAC,IAAI,CAAC;;IAE9B;IACAC,SAAS,CAAC,MAAM;MACdQ,cAAc,CAACC,KAAI,GAAIH,IAAI,CAACI,OAAO,CAACH,QAAQ,CAACE,KAAK,CAAC;IACrD,CAAC,CAAC;;IAEF;IACA,MAAME,sBAAqB,GAAIA,CAAA,KAAM;MACnCC,YAAY,CAACC,OAAO,CAAC,UAAU,EAAEN,QAAQ,CAACE,KAAK,CAAC;IAClD,CAAC;IAED,OAAO;MACLH,IAAI;MACJC,QAAQ;MACRC,cAAc;MACdG;IACF,CAAC;EACH,CAAC;EACDG,OAAOA,CAAA,EAAG;IACR;IACA,MAAMC,uBAAsB,GAAIH,YAAY,CAACI,OAAO,CAAC,UAAU,CAAC;IAChE,IACED,uBAAsB,IACtB,IAAI,CAACT,IAAI,CAACW,QAAQ,CAACF,uBAAuB,GAC1C;MACA,IAAI,CAACR,QAAO,GAAIQ,uBAAuB;IACzC;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}