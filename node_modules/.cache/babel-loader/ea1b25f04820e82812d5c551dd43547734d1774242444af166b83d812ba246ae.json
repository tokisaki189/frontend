{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nimport { useStore } from \"vuex\";\nimport Cookies from \"js-cookie\";\nimport jwtDecode from \"jwt-decode\";\nimport api from \"../api.js\";\nimport { mapMutations } from \"vuex\";\nexport default {\n  setup() {\n    let isLogin = ref(false);\n    let store = useStore();\n    let user = ref(\"\");\n    let userId = ref();\n    let userInfo = ref(\"\");\n    const {\n      setUserId\n    } = mapMutations([\"SetUserId\"]);\n    onMounted(() => {\n      // console.log(this.token.value);\n      api.get(\"/check\", {\n        withCredentials: true\n      }).then(response => {\n        if (response.data.isLogin === true) {\n          store.commit(\"login\");\n          isLogin.value = store.state.isLogin;\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n      const token = Cookies.get(\"token\");\n      if (token) {\n        user.value = jwtDecode(token);\n        userId.value = user.value.id;\n        console.log(userId.value);\n        setUserId;\n        api.get(\"/userprofile/\" + userId.value, {}).then(response => {\n          userInfo.value = response.data;\n          console.log(userInfo.value);\n        }).catch(error => {\n          console.log(error);\n        });\n      }\n    });\n    return {\n      isLogin,\n      user,\n      userId,\n      userInfo\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","useStore","Cookies","jwtDecode","api","mapMutations","setup","isLogin","store","user","userId","userInfo","setUserId","get","withCredentials","then","response","data","commit","value","state","catch","error","console","log","token","id"],"sources":["/home/prism/Documents/flyticket/src/views/UserProfile.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"user-profile\"></div>\n    <div v-if=\"userInfo\" class=\"user-info\">\n      <div>Username: {{ userInfo[0].username }}</div>\n      <div>First name: {{ userInfo[0].first_name }